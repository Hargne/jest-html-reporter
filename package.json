{
  "name": "jest-html-reporter",
  "version": "4.0.1",
  "description": "Jest test results processor for generating a summary in HTML",
  "main": "dist/index.js",
  "unpkg": "dist/index.js",
  "files": [
    "dist",
    "style"
  ],
  "scripts": {
    "tsc": "tsc",
    "lint": "eslint src/",
    "jest": "jest --no-cache --config=jest.config.json",
    "test": "npm run lint && npm run jest",
    "test:watch": "npm run jest -- --watch --no-coverage",
    "bundle": "rollup -c",
    "build": "npm run test && npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hargne/jest-html-reporter.git"
  },
  "keywords": [
    "jest",
    "html",
    "reporter",
    "report",
    "plugin"
  ],
  "author": "Johan Hargne",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Hargne/jest-html-reporter/issues"
  },
  "homepage": "https://github.com/Hargne/jest-html-reporter#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@jest/reporters": "^29.7.0",
    "@jest/test-result": "^29.7.0",
    "@jest/types": "^29.6.3",
    "dateformat": "3.0.2",
    "mkdirp": "^1.0.3",
    "strip-ansi": "6.0.1",
    "xmlbuilder": "15.0.0"
  },
  "peerDependencies": {
    "jest": "19.x - 29.x",
    "typescript": "^3.7.x || ^4.3.x || ^5.x"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@jest/console": "^29.7.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/dateformat": "^3.0.X",
    "@types/jest": "^29.5.14",
    "@types/mkdirp": "1.0.2",
    "@types/node": "^22.13.0",
    "@types/sinon": "9.0.11",
    "eslint": "^9.19.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup": "^4.34.1",
    "sinon": "^9.0.1",
    "ts-jest": "^29.0.0",
    "tslib": "^2.8.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.22.0"
  },
  "browserslist": [
    "since 2017-06"
  ],
  "packageManager": "yarn@1.22.19"
}
